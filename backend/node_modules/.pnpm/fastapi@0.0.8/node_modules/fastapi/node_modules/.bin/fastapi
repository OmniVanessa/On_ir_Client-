#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/vremicio/on_ir_client_vane/backend/node_modules/.pnpm/fastapi@0.0.8/node_modules/fastapi/lib/node_modules:/home/vremicio/on_ir_client_vane/backend/node_modules/.pnpm/fastapi@0.0.8/node_modules/fastapi/node_modules:/home/vremicio/on_ir_client_vane/backend/node_modules/.pnpm/fastapi@0.0.8/node_modules:/home/vremicio/on_ir_client_vane/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/vremicio/on_ir_client_vane/backend/node_modules/.pnpm/fastapi@0.0.8/node_modules/fastapi/lib/node_modules:/home/vremicio/on_ir_client_vane/backend/node_modules/.pnpm/fastapi@0.0.8/node_modules/fastapi/node_modules:/home/vremicio/on_ir_client_vane/backend/node_modules/.pnpm/fastapi@0.0.8/node_modules:/home/vremicio/on_ir_client_vane/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/cli.js" "$@"
else
  exec node  "$basedir/../../lib/cli.js" "$@"
fi
